{
  "name": "eas-cli",
  "description": "EAS command line tool",
  "version": "0.0.0",
  "author": "Expo <support@expo.io>",
  "bin": {
    "eas": "./bin/run"
  },
  "bugs": "https://github.com/expo/eas-cli/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^3",
    "tslib": "^1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@types/node": "^10",
    "ts-node": "^8",
    "typescript": "^3.9.7"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/expo/eas-cli",
  "keywords": [
    "expo",
    "eas",
    "cli"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "bin": "eas",
    "plugins": [
      "@eas/plugin-builds",
      "@eas/plugin-updates",
      "@eas/plugin-webhooks",
      "@oclif/plugin-help"
    ],
    "topics": {
      "build": { "description": "build app binaries" },
      "update": { "description": "roll out changes" },
      "webhook": { "description":  "manage webhooks" }
    }
  },
  "repository": "expo/eas-cli",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "echo NO TESTS",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
